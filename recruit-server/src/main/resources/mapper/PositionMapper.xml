        <result column="is_deleted" property="deleted" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        create_time,
        update_time,
        created_by,
        updated_by,
        id, name, salary, mission, code, p_code, requirement, hire_num, company_id, is_deleted
